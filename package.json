{
  "name": "pentonopolis",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm exec nx run-many -t build",
    
    "dependencies:check": "pnpm exec nx run-many -t dependencies:check && pnpm outdated --json > outdated.json || true",
    "lint:check": "pnpm exec nx run-many -t lint:check",
    "lint": "pnpm exec nx run-many -t lint",
    "mutate": "pnpm exec nx run-many -t mutate",
    "prepare": "husky",
    "prettier:check": "pnpm exec nx run-many -t prettier:check",
    "prettier": "pnpm exec nx run-many -t prettier",
    "release-major": "pnpm -r release-major",
    "release-minor": "pnpm -r release-minor",
    "release-patch": "pnpm -r release-patch",
    "test:compile": "pnpm exec nx run-many -t test:compile",
    "test:coverage": "pnpm exec nx run-many -t test:coverage",
    "test": "pnpm exec nx run-many -t test",
    "vulnerabilities:check": "pnpm audit --json > audit.json"
  },
  "devDependencies": {
    "@types/jest": "29.x",
    "@typescript-eslint/eslint-plugin": "7.x",
    "@typescript-eslint/parser": "7.x",
    "eslint": "8.x",
    "husky": "^9.0.11",
    "jest": "29.x",
    "prettier": "3.x",
    "typescript": "5.x",
    "nx": "19.x"
  },
